{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch (NodeJS)",
			"type": "node",
			"request": "launch",
			"preLaunchTask": "build",
			"cwd": "${workspaceRoot}/export/js/debug",
			"program": "${workspaceRoot}/export/js/debug/main.js"
		},
		{
			"name": "Attach (NodeJS)",
			"type": "node",
			"request": "attach",
			"processId": "${command:pickProcess}"
		}
	]
}
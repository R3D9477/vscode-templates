{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch (.NET Core)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "Build debug",
			"cwd": "${workspaceRoot}/bin/Debug/netcoreapp2.0",
			"program": "${workspaceRoot}/bin/Debug/netcoreapp2.0/test_console_application.dll",
			"console": "integratedTerminal"
		},
		{
			"name": "Attach (.NET Core)",
			"type": "coreclr",
			"request": "attach",
			"processId": "${command:pickProcess}"
		}
	]
}
{
	"version": "2.0.0",
	"tasks": [
		{
			"taskName": "Build debug",
			"command": "dotnet build --configuration Debug",
			"type": "shell",
			"group": "build",
			"problemMatcher": "$msCompile"
		},
		{
			"taskName": "Build release",
			"command": "dotnet build --configuration Release",
			"type": "shell",
			"group": "build",
			"problemMatcher": "$msCompile"
		}
	]
}